{
  "profiles": {
    "deploy .": {
      "commandName": "Project",
      "commandLineArgs": "deploy pgup.json --connection \"%DEV_POSTGRES_CONNECTION_STRING%\" --timeout 00:10:00"
    },
    "re-deploy .": {
      "commandName": "Project",
      "commandLineArgs": "deploy pgup.json --connection \"%DEV_POSTGRES_CONNECTION_STRING%\" --overwrite --force"
    }
  }
}